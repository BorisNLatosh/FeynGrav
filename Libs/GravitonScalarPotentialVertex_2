(-1/8*I)*\[Kappa]^2*\[Lambda]*Pair[LorentzIndex[m1, D], LorentzIndex[n2, D]]*
  Pair[LorentzIndex[m2, D], LorentzIndex[n1, D]] + 
 (I/8)*\[Kappa]^2*\[Lambda]*Pair[LorentzIndex[m1, D], LorentzIndex[n1, D]]*
  Pair[LorentzIndex[m2, D], LorentzIndex[n2, D]] - 
 (I/8)*\[Kappa]^2*\[Lambda]*Pair[LorentzIndex[m1, D], LorentzIndex[m2, D]]*
  Pair[LorentzIndex[n1, D], LorentzIndex[n2, D]]
