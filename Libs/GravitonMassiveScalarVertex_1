(-I/4)*m^2*\[Kappa]*Pair[LorentzIndex[m1, D], LorentzIndex[n1, D]] + 
 (I/4)*\[Kappa]*Pair[LorentzIndex[m1, D], Momentum[p2, D]]*
  Pair[LorentzIndex[n1, D], Momentum[p1, D]] + 
 (I/4)*\[Kappa]*Pair[LorentzIndex[m1, D], Momentum[p1, D]]*
  Pair[LorentzIndex[n1, D], Momentum[p2, D]] - 
 (I/4)*\[Kappa]*Pair[LorentzIndex[m1, D], LorentzIndex[n1, D]]*
  Pair[Momentum[p1, D], Momentum[p2, D]]
