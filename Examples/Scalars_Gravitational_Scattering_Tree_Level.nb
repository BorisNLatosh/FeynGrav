17351ab4e6a7"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"d\[Sigma]", "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{
        SqrtBox[
         RowBox[{
          SuperscriptBox["m1", "2"], "+", 
          SuperscriptBox["p", "2"]}]], " ", 
        SqrtBox[
         RowBox[{
          SuperscriptBox["m2", "2"], "+", 
          SuperscriptBox["p", "2"]}]]}], 
       SqrtBox[
        RowBox[{
         RowBox[{
          RowBox[{"-", 
           SuperscriptBox["m1", "2"]}], " ", 
          SuperscriptBox["m2", "2"]}], "+", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{
            SuperscriptBox["p", "2"], "+", 
            SqrtBox[
             RowBox[{
              RowBox[{"(", 
               RowBox[{
                SuperscriptBox["m1", "2"], "+", 
                SuperscriptBox["p", "2"]}], ")"}], " ", 
              RowBox[{"(", 
               RowBox[{
                SuperscriptBox["m2", "2"], "+", 
                SuperscriptBox["p", "2"]}], ")"}]}]]}], ")"}], "2"]}]]], 
      RowBox[{"Integrate", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"DiracDelta", "[", 
          RowBox[{
           SqrtBox[
            RowBox[{
             SuperscriptBox["m1", "2"], "+", 
             SuperscriptBox["p", "2"]}]], "+", 
           SqrtBox[
            RowBox[{
             SuperscriptBox["m2", "2"], "+", 
             SuperscriptBox["p", "2"]}]], "-", 
           SqrtBox[
            RowBox[{
             SuperscriptBox["m1", "2"], "+", 
             SuperscriptBox["k", "2"]}]], "-", 
           SqrtBox[
            RowBox[{
             SuperscriptBox["m2", "2"], "+", 
             SuperscriptBox["k", "2"]}]]}], "]"}], 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"2", "\[Pi]"}], ")"}], "4"], 
         FractionBox["1", 
          RowBox[{"16", 
           SqrtBox[
            RowBox[{
             SuperscriptBox["m1", "2"], "+", 
             SuperscriptBox["p", "2"]}]], 
           SqrtBox[
            RowBox[{
             SuperscriptBox["m2", "2"], "+", 
             SuperscriptBox["p", "2"]}]], 
           SqrtBox[
            RowBox[{
             SuperscriptBox["m1", "2"], "+", 
             SuperscriptBox["k", "2"]}]], 
           SqrtBox[
            RowBox[{
             SuperscriptBox["m2", "2"], "+", 
             SuperscriptBox["k", "2"]}]]}]], 
         SuperscriptBox["k", "2"], 
         SuperscriptBox[
          RowBox[{"(", 
           FractionBox["1", 
            SuperscriptBox[
             RowBox[{"(", 
              RowBox[{"2", "\[Pi]"}], ")"}], "3"]], ")"}], "2"]}], ",", 
        RowBox[{"{", 
         RowBox[{"k", ",", "0", ",", "\[Infinity]"}], "}"}]}], "]"}], 
      RowBox[{"ComplexConjugate", "[", "\[ScriptCapitalM]", "]"}], 
      "\[ScriptCapitalM]"}], "/.", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"s", "->", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{
           SqrtBox[
            RowBox[{
             SuperscriptBox["m1", "2"], "+", 
             SuperscriptBox["p", "2"]}]], "+", 
           SqrtBox[
            RowBox[{
             SuperscriptBox["m2", "2"], "+", 
             SuperscriptBox["p", "2"]}]]}], ")"}], "2"]}], ",", 
       RowBox[{"t", "->", 
        RowBox[{
         RowBox[{"-", "4"}], 
         SuperscriptBox["p", "2"], 
         SuperscriptBox[
          RowBox[{"Sin", "[", 
           FractionBox["\[Theta]", "2"], "]"}], "2"]}]}], ",", 
       RowBox[{"D", "->", "4"}], ",", 
       RowBox[{"\[Kappa]", "->", 
        SqrtBox[
         RowBox[{"32", " ", "\[Pi]", " ", "G"}]]}]}], "}"}]}]}], ";"}], "//", 
  "Timing"}]], "Input",
 Editable->False,
