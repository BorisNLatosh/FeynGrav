Cell[BoxData[
 RowBox[{
  RowBox[{"SetMandelstam", "[", 
   RowBox[{
   "s", ",", " ", "t", ",", " ", "u", ",", " ", "p1", ",", " ", "p2", ",", 
    " ", "p3", ",", " ", "p4", ",", " ", "m1", ",", " ", "m2", ",", " ", "m1",
