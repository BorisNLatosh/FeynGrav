Cell[TextData[StyleBox["The scattering matrix element.",
 FontWeight->"Bold"]], "Text",
 Editable->False,
 CellChangeTimes->{{3.894928800131402*^9, 
  3.894928805635047*^9}},ExpressionUUID->"2888fc82-e097-41be-8700-\
0f06d37d67e6"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"\[ScriptCapitalM]", "=", 
  RowBox[{
   RowBox[{
    RowBox[{"Calc", "[", 
     RowBox[{
      RowBox[{"GravitonScalarVertex", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"\[Mu]", ",", "\[Nu]"}], "}"}], ",", "p1", ",", "p3", ",", 
        "m1"}], "]"}], 
      RowBox[{"GravitonPropagatorAlternative", "[", 
       RowBox[{"\[Mu]", ",", "\[Nu]", ",", "\[Alpha]", ",", "\[Beta]", ",", 
        RowBox[{"p1", "+", "p3"}]}], "]"}], 
      RowBox[{"GravitonScalarVertex", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"\[Alpha]", ",", "\[Beta]"}], "}"}], ",", "p2", ",", "p4", 
        ",", "m2"}], "]"}]}], "]"}], "/.", 
    RowBox[{"u", "->", 
     RowBox[{
      RowBox[{"2", 
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["m1", "2"], "+", 
         SuperscriptBox["m2", "2"]}], ")"}]}], "-", "s", "-", "t"}]}]}], "//",
    "Simplify"}]}]], "Input",
 Editable->False,
 CellChangeTimes->{{3.886123852821093*^9, 3.886123907658071*^9}},
 CellLabel->"In[3]:=",ExpressionUUID->"493139ee-6c19-4a62-81b2-338479f23de0"],

Cell[BoxData[
 FormBox[
  FractionBox[
   RowBox[{"\[ImaginaryI]", " ", 
    SuperscriptBox["\[Kappa]", "2"], " ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       SuperscriptBox["D", "2"], " ", 
       SuperscriptBox["t", "2"]}], "+", 
      RowBox[{"4", " ", 
       SuperscriptBox["m1", "2"], " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{"D", "-", "2"}], ")"}], " ", "t"}], "+", 
         RowBox[{"4", " ", "s"}]}], ")"}]}], "+", 
      RowBox[{"4", " ", 
       SuperscriptBox["m2", "2"], " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{"D", "-", "2"}], ")"}], " ", "t"}], "+", 
         RowBox[{"4", " ", "s"}]}], ")"}]}], "-", 
      RowBox[{"6", " ", "D", " ", 
       SuperscriptBox["t", "2"]}], "-", 
      RowBox[{"8", " ", 
       SuperscriptBox["m1", "4"]}], "-", 
      RowBox[{"8", " ", 
       SuperscriptBox["m2", "4"]}], "-", 
      RowBox[{"8", " ", 
       SuperscriptBox["s", "2"]}], "-", 
      RowBox[{"8", " ", "s", " ", "t"}], "+", 
      RowBox[{"8", " ", 
       SuperscriptBox["t", "2"]}]}], ")"}]}], 
   RowBox[{"32", " ", "t"}]], TraditionalForm]], "Output",
 CellChangeTimes->{3.894928787962043*^9, 3.923513465647104*^9, 
  3.923517243486702*^9, 3.923517300119523*^9},
