Cell[TextData[StyleBox["Here an amplitude for the s-channel scattering is \
evaluated.\nThe incoming gravitons have momenta p1 and p2. Their lines have \
the indices \[Mu]1,\[Nu]2 and \[Mu]2,\[Nu]2 respectively.\nThe outgoing \
gravitons have momenta p3 and p4. Their lines have the indices \[Mu]3,\[Nu]3 \
and \[Mu]4,\[Nu]4 respectively.\nThe graviton propagator connects vertices \
with indices \[Alpha]1,\[Beta]1 and \[Alpha]2,\[Beta]2 respectively. It \
carries momenta p1+p2.",
 FontWeight->"Bold"]], "Text",
 Editable->False,
 CellChangeTimes->{{3.894592569797389*^9, 
  3.894592570650181*^9}},ExpressionUUID->"c04d49e1-8ef0-4074-be6e-\
746dfdf0df1d"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"\[ScriptCapitalM]s", "[", 
     RowBox[{
     "\[Mu]1_", ",", "\[Nu]1_", ",", "\[Mu]2_", ",", "\[Nu]2_", ",", 
      "\[Mu]3_", ",", "\[Nu]3_", ",", "\[Mu]4_", ",", "\[Nu]4_", ",", "p1_", 
      ",", "p2_", ",", "p3_", ",", "p4_"}], "]"}], "=", 
    RowBox[{"Contract", "[", 
     RowBox[{
      RowBox[{"GravitonVertex", "[", 
       RowBox[{
       "\[Mu]1", ",", "\[Nu]1", ",", "p1", ",", "\[Mu]2", ",", "\[Nu]2", ",", 
        "p2", ",", "\[Alpha]1", ",", "\[Beta]1", ",", 
        RowBox[{
         RowBox[{"-", "p1"}], "-", "p2"}]}], "]"}], 
      RowBox[{"GravitonPropagatorAlternative", "[", 
       RowBox[{
       "\[Alpha]1", ",", "\[Beta]1", ",", "\[Alpha]2", ",", "\[Beta]2", ",", 
        RowBox[{"p1", "+", "p2"}]}], "]"}], 
      RowBox[{"GravitonVertex", "[", 
       RowBox[{
       "\[Mu]3", ",", "\[Nu]3", ",", "p3", ",", "\[Mu]4", ",", "\[Nu]4", ",", 
        "p4", ",", "\[Alpha]2", ",", "\[Beta]2", ",", 
        RowBox[{"p1", "+", "p2"}]}], "]"}]}], "]"}]}], ";"}], "//", 
  "Timing"}]], "Input",
 Editable->False,
 CellChangeTimes->{{3.894586144995569*^9, 3.894586148315744*^9}, {
   3.894586693328663*^9, 3.8945866969999533`*^9}, 3.894587520266344*^9},
 CellLabel->"In[2]:=",ExpressionUUID->"1cd44712-aff8-4e5c-b996-7121ed79c9f9"],

Cell[BoxData[
